datasource hammerDatasource {
  provider = env("PRISMA_DS_PROVIDER") // sqlite
  url = env("PRISMA_DS_URL") // "file:./dev.db"
}

generator photonjs {
  provider = "photonjs"
  output = "../../packages/photon"
  binaryTargets = ["darwin", "linux-glibc-libssl1.0.2"]
}

// Define your own datamodels here and run `yarn db:save` to create
// migrations for them.

model User {
  id Int @id
  name String?
  email String @unique
  isAdmin Boolean @default(false)
}
