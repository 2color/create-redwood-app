datasource hammerDatasource {
  provider = "sqlite"
  url = "file:./dev.db"
}

generator photonjs {
  provider = "photonjs"
  output = "../../packages/photon"
}

// Define your own datamodels here and run `yarn db:save` to create
// migrations for them.

model User {
  id Int @id
  name String?
  email String @unique
  isAdmin Boolean @default(false)
}
